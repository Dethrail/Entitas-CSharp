    <PackageTags>Entitas, ECS, Unity, Entity, Component, System</PackageTags>
    <RepositoryUrl>https://github.com/sschmid/Entitas-CSharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

</Project>
